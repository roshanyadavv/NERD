#include<iostream>
using namespace std;
int main()
{
    int n,i,j,s=1;
    cin>>n;
    int a[n][n];
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            cin>>a[i][j];
        }
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            if((a[i][j]!=20)&&(a[i][j]>10)){s=0;}
        }
    }
    (s==0)?cout<<"No":cout<<"Yes";
    return 0;
}