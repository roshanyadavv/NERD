#include<bits/stdc++.h>
using namespace std;
int main()
{
    int s1,s2,s3,s4,s5;
    float avg=0;
    cin>>s1>>s2>>s3>>s4>>s5;
    avg=(s1+s2+s3+s4+s5)/5;
    if((s1>2)&&(s2>2)&&(s3>2)&&(s4>2)&&(s5>2))
    {
        if((s1==5)||(s2==5)||(s3==5)||(s4==5)||(s5==5))
        {
            (avg>=4)?cout<<"Yes":cout<<"No";
        }
        else
        {
            cout<<"No";
        }
    }
    else
    {
        cout<<"No";
    }
    return 0;
}