#include<iostream>
using namespace std;
int main()
{
    int a,b,c;
    char x,y,z;
    cin>>x;
    cin>>a;
    cin>>y;
    cin>>b;
    cin>>z;
    cin>>c;
    if(((x==y)&&(y==z))&&((a==b)&&(a==c)))
    {
        cout<<"Double Bonanza";
    }
    else if(((x==y)&&(x==z))||((a==b)&&(a==c)))
    {
           cout<<"Bonanza";
    }
    else
    {
        cout<<"No Bonanza";
    }
    return 0;
}